%start VM_ArmingMap::Init
%class VM_Arming
%fsmclass VM_ArmingContext
%header vm_Arming.h
%map VM_ArmingMap

%%
Init
    Entry
    {
	    EnteredInit();
    }
{
    InitComplete  Standby {}
}
Standby
    Entry
    {
	    EnteredStandby();
	}
{
    Arm           [
    			  		ctxt.PreFlightCheckCleared() == true
    			  ]
    			  Armed {}
    Error         StandbyError {}
}
Armed
    Entry
    {
        EnteredArmed();
    }
    Exit
    {
        ExitedArmed();
    }
{
    Disarm        Standby {}
    Error         ArmedError {}
}
StandbyError
    Entry
    {
        EnteredStandbyError();
    }
{
}
ArmedError
    Entry
    {
        EnteredArmedError();
    }
{
}
%%
