%start VM_ArmingFSM::Init
%header VM_Arming.h
%class VM_Arming

%map VM_ArmingFSM
%%
ArmedError
Entry {SetArmed(false);}
{
}

StandbyError
Entry {SetArmed(false);}
{
}

Armed
Entry {SetArmed(true);}
{
	Disarm	Standby	{}
	Error	ArmedError	{}
	Reset	Init	{}
}

Standby
Entry {SetArmed(false);}
{
	Arm	Armed	{}
	Error	StandbyError	{}
	Reset	Init	{}
}

Init
Entry {SetArmed(false);}
{
	InitComplete	Standby	{}
	Reset	Init	{}
}

%%
