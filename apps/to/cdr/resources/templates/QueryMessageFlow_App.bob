<?xml version="1.0" encoding="UTF-8"?>
<display version="2.0.0">
  <name>($(CPUID)) TO - Query Msg Flow</name>
  <macros>
    <CPUID>{{ cpu_id }}</CPUID>
@BUILDLINER_CDR_MACRO_BLOCK@
@BUILDLINER_CDR_MACRO_BLOCK_CUSTOM@
  </macros>
  <width>420</width>
  <height>200</height>
  <actions>
  </actions>
  <scripts>
    <script file="../scripts/telemetry_management_init.py">
      <pv_name>=1</pv_name>
    </script>
  </scripts>
  <grid_step_x>6</grid_step_x>
  <grid_step_y>6</grid_step_y>
  <widget type="label" version="2.0.0">
    <name>Label_49</name>
    <text>($(CPUID) - $(APP)) TO - Query Message Flow</text>
    <x>30</x>
    <y>20</y>
    <width>313</width>
    <font>
      <font name="Default Bold" family="Liberation Sans" style="BOLD" size="14.0">
      </font>
    </font>
    <horizontal_alignment>1</horizontal_alignment>
    <vertical_alignment>1</vertical_alignment>
    <wrap_words>false</wrap_words>
    <actions>
    </actions>
    <border_color>
      <color red="0" green="128" blue="255">
      </color>
    </border_color>
  </widget>
  <widget type="label" version="2.0.0">
    <name>Label</name>
    <text>Message ID</text>
    <x>30</x>
    <y>50</y>
    <width>90</width>
    <horizontal_alignment>2</horizontal_alignment>
    <vertical_alignment>1</vertical_alignment>
    <wrap_words>false</wrap_words>
    <actions>
    </actions>
    <border_color>
      <color red="0" green="128" blue="255">
      </color>
    </border_color>
  </widget>
  <widget type="action_button" version="3.0.0">
    <name>NoOp</name>
    <actions>
      <action type="execute">
        <script file="../scripts/query_message_app.py">
        </script>
      </action>
    </actions>
    <text>Query</text>
    <x>150</x>
    <y>122</y>
    <width>84</width>
    <height>20</height>
    <foreground_color>
      <color red="253" green="255" blue="255">
      </color>
    </foreground_color>
    <background_color>
      <color red="34" green="45" blue="75">
      </color>
    </background_color>
    <tooltip>$(actions)</tooltip>
    <border_alarm_sensitive>false</border_alarm_sensitive>
  </widget>
  <widget type="combo" version="2.0.0">
    <name>msgIdInput</name>
    <pv_name>loc://$(APP)_$(CPUID)_TO_QueryMessageFlow_MsgID_App&lt;VString&gt;</pv_name>
    <x>126</x>
    <y>50</y>
    <width>210</width>
    <height>20</height>
    <background_color>
      <color red="255" green="255" blue="255">
      </color>
    </background_color>
    <actions>
    </actions>
    <scripts>
      <script file="../scripts/set_msg_id.py">
        <pv_name>loc://$(APP)_$(CPUID)_TO_QueryMessageFlow_MsgID_App&lt;VString&gt;</pv_name>
      </script>
      <script file="../scripts/telemetry_management_init.py">
        <pv_name>loc://$(APP)_$(CPUID)_TO_QueryMessageFlow_MsgID_App&lt;VString&gt;</pv_name>
      </script>
    </scripts>
    <items_from_pv>false</items_from_pv>
  </widget>
  <widget type="label" version="2.0.0">
    <name>Label_4</name>
    <text>Channel</text>
    <x>30</x>
    <y>72</y>
    <width>90</width>
    <horizontal_alignment>2</horizontal_alignment>
    <vertical_alignment>1</vertical_alignment>
    <wrap_words>false</wrap_words>
    <actions>
    </actions>
    <border_color>
      <color red="0" green="128" blue="255">
      </color>
    </border_color>
  </widget>
  <widget type="spinner" version="2.0.0">
    <name>ChannelIndex</name>
    <pv_name>loc://$(APP)_$(CPUID)_TO_QueryMessgaeFlow_ChannelIndex_App</pv_name>
    <x>126</x>
    <y>72</y>
    <width>210</width>
    <format>0</format>
    <precision>0</precision>
    <background_color>
      <color red="255" green="255" blue="255">
      </color>
    </background_color>
    <actions>
    </actions>
    <maximum>10.0</maximum>
    <limits_from_pv>false</limits_from_pv>
  </widget>
</display>
