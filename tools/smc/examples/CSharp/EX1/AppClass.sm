%start Map1::Start
%class AppClass

%map Map1
%%
// State		Transition		End State		Action(s)
Start {
				Zero			Zeros			{}
				One				Ones			{}
				Unknown			Error			{}
				EOS				OK				{
                                                    Acceptable = true;
                                                }
}

Zeros {
				Zero			Zeros			{}
				One				Ones			{}
				Unknown			Error			{}
				EOS				OK				{
													Acceptable = true; 
												}
}

Ones {
				Zero			Error			{}
				One				Ones			{}
				Unknown			Error			{}
				EOS				OK				{
                                                    Acceptable=true;
                                                    AcceptableM(true);
                                                }
}

OK {}

Error 
Entry {SignalError();}
{
				Zero			nil				{}
				One				nil				{}
				Unknown			nil				{}
				EOS				nil				{Acceptable=false;}//Unacceptable();}
}

%%
